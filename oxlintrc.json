{
  "$schema": "./node_modules/oxlint/configuration_schema.json",
  "plugins": ["import", "jsx-a11y", "react-perf", "node", "security"],
  "env": {
    "browser": true,
    "node": true,
    "es2024": true
  },
  "rules": {
    "import/no-cycle": "warn",
    "no-unused-vars": "warn",
    "no-img-element": "off"
  }
}
